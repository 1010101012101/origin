ROLE="node"
OPTIONS="--loglevel=0"
# If running node on a separate host please rsync the admin kubeconfig ie:
# rsync root@openshift-master:/var/lib/openshift/openshift.local.config/master/admin.kubeconfig /var/lib/openshift/openshift.local.config/master
KUBECONFIG="/var/lib/openshift/openshift.local.config/master/admin.kubeconfig"
IMAGES='openshift/origin-${component}:${version}'
